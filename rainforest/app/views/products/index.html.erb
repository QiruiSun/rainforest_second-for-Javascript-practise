<h1>Products Show</h1>
<!-- <p>Find me in app/views/products/index.html.erb</p> -->

<h3><%= link_to "Add one", new_product_path %></h3>

<%= form_tag products_path, method: :get, authentication: false, id: 'search-form' do %>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search" %>
<% end %>

<div id="products">
  <%= render @products %>
</div>
